        from {
          opacity: 0;
        }
        to {
          opacity: 1;
        }
      }
      @keyframes slideUp {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
      .coffee-steam {
        animation: float 3s ease-in-out infinite;
      }
      .coffee-steam:nth-child(2) {
        animation-delay: 0.5s;
      }
      .coffee-steam:nth-child(3) {
        animation-delay: 1s;
      }
