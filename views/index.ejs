    <script>
      // Smooth scrolling for navigation links
      document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
        anchor.addEventListener("click", function (e) {
          e.preventDefault();
          const target = document.querySelector(this.getAttribute("href"));
          if (target) {
            target.scrollIntoView({
              behavior: "smooth",
              block: "start",
            });
          }
        });
      });

      // Add scroll effect to navbar
      window.addEventListener("scroll", function () {
        const navbar = document.querySelector("nav");
        if (window.scrollY > 50) {
          navbar.classList.add("bg-white/95");
          navbar.classList.remove("bg-white/90");
        } else {
          navbar.classList.add("bg-white/90");
          navbar.classList.remove("bg-white/95");
        }
      });

      // Button click handlers
      document.querySelectorAll("button").forEach((button) => {
        button.addEventListener("click", function () {
          const buttonText = this.textContent.trim();

          if (
            buttonText.includes("Get Started") ||
            buttonText.includes("Start")
          ) {
            alert(
              "Welcome to EduVerse! Sign-up functionality would be implemented here."
            );
          } else if (
            buttonText.includes("Demo") ||
            buttonText.includes("Watch Demo")
          ) {
            alert("Demo video would play here. Experience EduVerse in action!");
          } else if (buttonText.includes("Contact Sales")) {
            alert(
              "Sales team contact form would open here. Let us help you choose the perfect plan!"
            );
          } else if (buttonText.includes("Schedule Demo")) {
            alert(
              "Demo scheduling calendar would open here. Book your personalized demo!"
            );
          } else if (buttonText.includes("Free Trial")) {
            alert(
              "Free trial registration would start here. Try EduVerse risk-free for 14 days!"
            );
          }
        });
      });
    </script>
